        <idmp:isUMLAttributeOf rdf:resource="https://purl.iso.org/idmp/StructuralModification"/>
        <skos:definition>The Type of structural modification should be described.</skos:definition>
        <skos:example>Amino Acid Substitution of the peptide/protein, Salt, hydrate, solvate formation, chemical substitution, moiety, amino acid sulphation, nucleic acid base substitution, Reductive Amination</skos:example>
        <skos:prefLabel>structural modification type</skos:prefLabel>
    </rdf:Description>
    <rdf:Description rdf:about="https://purl.iso.org/idmp/StructuralRepeat">
