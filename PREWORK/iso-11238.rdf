    <rdf:Description rdf:about="https://purl.iso.org/idmp/StructuralRepeatUnitValue">
        <terms:source rdf:resource="https://purl.iso.org/idmp/iso-ts-19844/2018E"/>
        <skos:definition>Structural repeat units are essential elements for defining polymers. Specific guidance will be given in the /Polymer.</skos:definition>
        <idmp:businessRules>End-groups if defined can be attached to the Structural Repeat Unit. The Structural Repeat Unit shall be the largest repeating structural fragment</idmp:businessRules>
        <terms:source>ISO/TS 19844:2018(E) page 98</terms:source>
        <idmp:isUMLAttributeOf rdf:resource="https://purl.iso.org/idmp/StructuralRepeatUnit"/>
