    <rdf:Description rdf:about="https://purl.iso.org/idmp/ReferenceSourceCitation">
        <rdfs:seeAlso rdf:resource="https://purl.iso.org/idmp/777958EA-F11D-4327-A155-974E4D3D8644"/>
        <skos:altLabel>IDMP.referenceSource.referenceSourceCitation</skos:altLabel>
