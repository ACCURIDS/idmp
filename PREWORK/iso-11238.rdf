        <idmp:isUMLAttributeOf rdf:resource="https://purl.iso.org/idmp/NucleicAcidSubunit"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://purl.iso.org/idmp/Sugar">
        <idmp:hasUMLAttribute rdf:resource="https://purl.iso.org/idmp/SugarIdentifier"/>
        <skos:prefLabel>sugar</skos:prefLabel>
        <skos:altLabel>sugars</skos:altLabel>
        <skos:definition>Sugar or sugar-like element is one of the three elements that define a nucleic acid.</skos:definition>
        <rdfs:seeAlso rdf:resource="https://purl.iso.org/idmp/33C03E10-5530-4FE5-BF06-A0D595E5FC71"/>
        <idmp:hasUMLAttribute rdf:resource="https://purl.iso.org/idmp/SugarName"/>
