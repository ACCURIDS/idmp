        <http://purl.org/dc/terms/source>
                "ISO/TS 19844:2018(E) page 96" ;
        <http://purl.org/dc/terms/source>
                <https://purl.iso.org/idmp/iso-ts-19844/2018E> ;
        <http://www.w3.org/2004/02/skos/core#altLabel>
                "structural repeats" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This subclause specifies and quantifies the repeated units and their configuration. A structural representation shall also be submitted. Information on the structural repeat unit shall be provided by means of the following data elements." ;
        <http://www.w3.org/2004/02/skos/core#narrower>
                <https://purl.iso.org/idmp/StructuralRepeatUnitAmountType> , <https://purl.iso.org/idmp/NumberOfStructuralRepeatUnits> , <https://purl.iso.org/idmp/AverageMolecularFormulaByRepearUnit> ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "structural repeat" ;
        <http://www.w3.org/2004/02/skos/core#related>
                <https://purl.iso.org/idmp/StructuralRepeatUnit> ;
        <https://purl.iso.org/idmp/conformance>
