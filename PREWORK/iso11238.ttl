                "Chemical substance, Protein Substance, Herbal Substance" , "Chemical substance, Biological substance" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "starting material type" ;
        <https://purl.iso.org/idmp/conformance>
                <https://purl.iso.org/idmp/MandatoryInformation> .

<https://purl.iso.org/idmp/AmountNonNumericValue>
