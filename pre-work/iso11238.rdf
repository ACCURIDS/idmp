                                                  <http://www.w3.org/2004/02/skos/core#example> "random, alternating, block, graft, statistical, periodic, mixed, cross, N/A" ;
                                                  <http://www.w3.org/2004/02/skos/core#altLabel> "copolymer connectivities" ;
                                                  <http://purl.org/dc/terms/source> "ISO/TS 19844:2018(E) page 94" .


