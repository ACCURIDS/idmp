                                                  <http://purl.org/dc/terms/source> "ISO 11238:2018(E) page 52" ;
                                                  <http://www.w3.org/2004/02/skos/core#prefLabel> "specification category" ;
                                                  <http://purl.org/dc/terms/source> <https://purl.iso.org/idmp/iso-11238/2018E> .


<https://purl.iso.org/idmp/SpecificationType> <http://www.w3.org/2004/02/skos/core#prefLabel> "specification type" ;
